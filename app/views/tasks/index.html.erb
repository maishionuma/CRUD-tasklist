<h1>タスクリスト一覧</h1>

<ul>
  <% @tasks.each do |task| %>
    <li><%= link_to task.id, task %> : <%= task.status %> &gt; <%= task.content %></li>
  <% end %>
</ul>

<%= link_to '新規タスクリストの投稿', new_task_path %>

<div class="center jumbotron">
  <div class="text-center">
    <h1>タスクリストにログイン</h1>
    <%= link_to 'Sign up', signup_path, class: 'btn btn-lg btn-primary' %>
  </div>
</div>